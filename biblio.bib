@mvbook{debret:1834voyage,
  title = {Voyage pittoresque et historique au Brésil: ou, Séjour d'un Artiste Français au Brésil, depuis 1816 jusqu'en 1831 inclusivement, époques de l'Avenement et de l'Abdication de S. M. D. Pedro Ier, fondateur de l'Empire brésilien},
